{
  "name": "weconnect",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.19.0",
    "fetch-mock": "^7.5.1",
    "history": "^4.10.1",
    "jw-paginate": "^1.0.4",
    "jw-react-pagination": "^1.1.0",
    "nock": "^11.5.0",
    "react": "^16.11.0",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^16.11.0",
    "react-icons": "^3.7.0",
    "react-redux": "^7.1.1",
    "react-router-dom": "^5.1.2",
    "react-router-redux": "^4.0.8",
    "react-scripts": "3.2.0",
    "redux": "^4.0.4",
    "redux-starter-kit": "^0.9.1",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "coveralls": "./node_modules/.bin/jest --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "eject": "react-scripts eject",
    "prettier": "prettier --write test/**/*.test.js src/**/*.js src/**/**/*.js ",
    "lint": "eslint --debug src/ test/",
    "lint:write": "eslint --debug src/ test/ --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "jest": {
    "setupFiles": [
      "./test/jestsetup.js",
      "jest-localstorage-mock"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "transformIgnorePatterns": [
      "/node_modules/(?!(jest-test))"
    ],
    "collectCoverage": true,
    "unmockedModulePathPatterns": [
      "react"
    ],
    "moduleNameMapper": {
      "\\.(css|less|scss|sss|styl)$": "<rootDir>/node_modules/jest-css-modules"
    }
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "coveralls": "^3.0.7",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "enzyme-to-json": "^3.4.3",
    "eslint": "^6.5.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-jest-enzyme": "^7.1.1",
    "eslint-config-react-app": "^5.0.2",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-flowtype": "^4.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "husky": "^3.0.9",
    "jest": "^24.9.0",
    "jest-css-modules": "^2.1.0",
    "jest-localstorage-mock": "^2.4.0",
    "moxios": "^0.4.0",
    "prettier": "^1.18.2",
    "pretty-quick": "^2.0.0",
    "prop-types": "^15.7.2",
    "react-mock-router": "^1.0.15",
    "react-test-renderer": "^16.11.0",
    "redux-mock-store": "^1.5.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
